{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/master-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/master-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/master-api",
  "projects": {
    "master-api": {
      "type": "application",
      "root": "apps/master-api",
      "entryFile": "main",
      "sourceRoot": "apps/master-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/master-api/tsconfig.app.json"
      }
    },
    "reader": {
      "type": "application",
      "root": "apps/reader",
      "entryFile": "main",
      "sourceRoot": "apps/reader/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reader/tsconfig.app.json"
      }
    },
    "data-modeling": {
      "type": "library",
      "root": "libs/data-modeling",
      "entryFile": "index",
      "sourceRoot": "libs/data-modeling/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-modeling/tsconfig.lib.json"
      }
    }
  }
}